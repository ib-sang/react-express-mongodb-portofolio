{
  "name": "react-express-mongodb-portofolio",
  "version": "1.0.0",
  "description": "Express Node JS applicqtion server portofolio with react js",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack serve --mode development --color --config build/webpack.base.config.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
